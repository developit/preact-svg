{
  "name": "preact-svg",
  "version": "1.4.1",
  "description": "Use SVG with Preact.",
  "main": "dist/preact-svg.js",
  "scripts": {
    "build": "babel -f $npm_package_main -s -o $npm_package_main < src/index.js",
    "prebuild": "mkdirp dist",
    "test": "eslint {src,test} && mocha --compilers js:babel/register test/**/*.js",
    "prepublish": "npm run build",
    "release": "npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "keywords": [
    "preact",
    "svg"
  ],
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/developit/preact-svg.git"
  },
  "bugs": {
    "url": "https://github.com/developit/preact-svg/issues"
  },
  "homepage": "https://github.com/developit/preact-svg",
  "peerDependencies": {
    "preact": "^1.5.0 || ^2.1.0 || ^3"
  },
  "optionalDependencies": {
    "preact": "^3.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^5.0.0",
    "chai": "^3.3.0",
    "eslint": "^2.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.3.3",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "preact": "^3.2.0"
  }
}
